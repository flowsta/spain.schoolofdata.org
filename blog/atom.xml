---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>School of Data España</title>
	<link href="http://flowsta.github.io/spain.schoolofdata.org/blog/atom.xml" rel="self"/>
	<link href="http://flowsta.github.io/spain.schoolofdata.org/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://flowsta.github.io/spain.schoolofdata.org/blog</id>
	<author>
		<name>School of Data España</name>
		<email>scoda@okfn.es</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://flowsta.github.io/spain.schoolofdata.org{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://flowsta.github.io/spain.schoolofdata.org/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
